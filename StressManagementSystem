    {
      "cell_type": "code",
      "source": [
        "import os\n",
        "from langchain.vectorstores import Chroma\n",
        "from langchain.document_loaders import DirectoryLoader, PyPDFLoader\n",
        "from langchain.text_splitter import RecursiveCharacterTextSplitter\n",
        "from langchain.embeddings import HuggingFaceBgeEmbeddings\n",
        "from langchain.chains import RetrievalQA\n",
        "from langchain.prompts import PromptTemplate\n",
        "from langchain_groq import ChatGroq\n",
        "\n",
        "# ✅ Function to initialize LLM\n",
        "def initialize_llm():\n",
        "    print(\"🔄 Initializing LLM...\")\n",
        "    llm = ChatGroq(\n",
        "        temperature=0,\n",
        "        groq_api_key=\"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\",  # 🔴 Replace with your actual API key!\n",
        "        model_name=\"llama3-8b-8192\"\n",
        "    )\n",
        "    print(\"✅ LLM initialized successfully.\")\n",
        "    return llm\n",
        "\n",
        "# ✅ Function to create ChromaDB vector store\n",
        "def create_vector_db():\n",
        "    print(\"📂 Loading PDFs from /content/data/...\")\n",
        "    loader = DirectoryLoader(\"/content/data/\", glob=\"*.pdf\", loader_cls=PyPDFLoader)\n",
        "    documents = loader.load()\n",
        "\n",
        "    if not documents:\n",
        "        print(\"⚠️ No documents found in /content/data/. Check the directory!\")\n",
        "        return None\n",
        "\n",
        "    print(f\"✅ Loaded {len(documents)} documents.\")\n",
        "\n",
        "    # Split text for better retrieval\n",
        "    text_splitter = RecursiveCharacterTextSplitter(chunk_size=500, chunk_overlap=50)\n",
        "    texts = text_splitter.split_documents(documents)\n",
        "\n",
        "    # Generate embeddings\n",
        "    embeddings = HuggingFaceBgeEmbeddings(model_name=\"sentence-transformers/all-MiniLM-L6-v2\")\n",
        "\n",
        "    # Create ChromaDB\n",
        "    vector_db = Chroma.from_documents(texts, embeddings, persist_directory=\"./chroma_db\")\n",
        "    vector_db.persist()\n",
        "\n",
        "    print(\"✅ ChromaDB created and saved.\")\n",
        "    return vector_db\n",
        "\n",
        "# ✅ Function to set up QA Chain\n",
        "def setup_qa_chain(vector_db, llm):\n",
        "    print(\"🔄 Setting up QA Chain...\")\n",
        "    retriever = vector_db.as_retriever()\n",
        "\n",
        "    prompt_template = \"\"\"You are a stress management system. Respond thoughtfully to the following question:\n",
        "    {context}\n",
        "    User: {question}\n",
        "    Assistant:\"\"\"\n",
        "\n",
        "    prompt = PromptTemplate(template=prompt_template, input_variables=['context', 'question'])\n",
        "\n",
        "    qa_chain = RetrievalQA.from_chain_type(\n",
        "        llm=llm,\n",
        "        chain_type=\"stuff\",\n",
        "        retriever=retriever,\n",
        "        chain_type_kwargs={'prompt': prompt}\n",
        "    )\n",
        "\n",
        "    print(\"✅ QA Chain setup complete.\")\n",
        "    return qa_chain\n",
        "\n",
        "# ✅ Main function\n",
        "def main():\n",
        "    print(\"🚀 Initializing Assistant...\")\n",
        "\n",
        "    # Initialize LLM\n",
        "    llm = initialize_llm()\n",
        "\n",
        "    # Check if ChromaDB exists\n",
        "    db_path = \"./chroma_db\"\n",
        "\n",
        "    if not os.path.exists(db_path + \"/index\"):  # ✅ Fixed path check\n",
        "        print(\"⚠️ ChromaDB not found, creating a new one...\")\n",
        "        vector_db = create_vector_db()\n",
        "        if vector_db is None:\n",
        "            print(\"❌ Could not create ChromaDB. Exiting...\")\n",
        "            return\n",
        "    else:\n",
        "        print(\"✅ Loading existing ChromaDB...\")\n",
        "        embeddings = HuggingFaceBgeEmbeddings(model_name=\"sentence-transformers/all-MiniLM-L6-v2\")\n",
        "        vector_db = Chroma(persist_directory=db_path, embedding_function=embeddings)\n",
        "\n",
        "    # ✅ Ensure `qa_chain` is properly created\n",
        "    qa_chain = setup_qa_chain(vector_db, llm)\n",
        "\n",
        "    # ✅ Chatbot loop inside `main()`\n",
        "    while True:\n",
        "        query = input(\"\\nHuman: \")\n",
        "        if query.lower() == \"exit\":\n",
        "            print(\"Assistant: Take care of yourself, Goodbye! 👋\")\n",
        "            break\n",
        "\n",
        "        print(\"🔄 Processing query...\")  # Debugging step\n",
        "        try:\n",
        "            response = qa_chain.run(query)  # 🔴 Now `qa_chain` is properly defined\n",
        "            print(f\"Assistant: {response}\")\n",
        "        except Exception as e:\n",
        "            print(f\"❌ Error occurred: {e}\")  # Catch any errors\n",
        "\n",
        "# ✅ Run the chatbot\n",
        "if __name__ == \"__main__\":\n",
        "    main()"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
